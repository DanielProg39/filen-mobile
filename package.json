{
  "name": "filen-mobile",
  "version": "2.0.50",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clear-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*",
    "dist-android-clean": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && cd ./android && ./gradlew bundleRelease && ./gradlew assembleRelease && cd ..",
    "dist-android": "cd ./android && ./gradlew bundleRelease && ./gradlew assembleRelease && cd ..",
    "splash": "react-native generate-bootsplash bootsplash.png --background-color=#263179 --logo-width=100 --flavor=main --assets-path=src/assets",
    "icons": "react-native-svg-app-icon",
    "install-release-apk-op": "adb -s 6c7a7e7b install android/app/build/outputs/apk/release/app-release.apk",
    "install-release-apk-op2": "adb -s 1c6c9bc3 install android/app/build/outputs/apk/release/app-release.apk",
    "install-release-apk-sm": "adb -s RZCW20AMCBB install android/app/build/outputs/apk/release/app-release.apk",
    "install-release-apk-emulator": "adb -s emulator-5554 install android/app/build/outputs/apk/release/app-release.apk",
    "android-release": "react-native run-android --variant=release deviceId=6c7a7e7b",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write"
  },
  "dependencies": {
    "@dudigital/react-native-zoomable-view": "1.1.3",
    "@react-native-community/hooks": "^2.8.0",
    "@react-native-community/netinfo": "^9.0.0",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "6.5.2",
    "@react-navigation/stack": "^6.2.0",
    "@rivascva/react-native-code-editor": "1.2.2",
    "@sentry/react-native": "4.13.0",
    "@shopify/flash-list": "1.4.0",
    "axios": "1.2.2",
    "crypto-js": "4.1.1",
    "expo": "^48.0.6",
    "expo-clipboard": "~4.1.2",
    "expo-file-system": "~15.2.2",
    "expo-font": "~11.1.1",
    "expo-keep-awake": "~12.0.1",
    "expo-local-authentication": "~13.2.1",
    "expo-media-library": "~15.2.2",
    "expo-navigation-bar": "~2.1.1",
    "expo-network": "~5.2.1",
    "expo-status-bar": "~1.4.4",
    "expo-video-thumbnails": "~7.2.1",
    "graceful-fs": "4.2.9",
    "intl": "1.2.5",
    "lodash": "4.17.21",
    "mime-types": "2.1.34",
    "nodejs-mobile-react-native": "github:Dwynr/nodejs-mobile-react-native",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.3",
    "react-native-actions-sheet": "^0.8.29",
    "react-native-blob-util": "^0.17.2",
    "react-native-device-info": "8.4.8",
    "react-native-dialog": "^9.3.0",
    "react-native-document-picker": "8.1.1",
    "react-native-fast-image": "8.6.3",
    "react-native-file-viewer": "^2.1.5",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-heic-converter": "1.3.1",
    "react-native-image-picker": "^5.3.1",
    "react-native-image-resizer": "1.4.5",
    "react-native-ios-asset-exporter": "0.1.6",
    "react-native-mmkv": "^2.6.3",
    "react-native-permissions": "^3.8.0",
    "react-native-picker-select": "8.0.4",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-share-menu": "github:meedan/react-native-share-menu",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "13.4.0",
    "react-native-toast-notifications": "^3.3.1",
    "react-native-windows": "^0.71.0",
    "react-use": "17.3.2",
    "striptags": "3.2.0",
    "uuid": "9.0.0",
    "zustand": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "7.1.6",
    "@babel/runtime": "7.12.5",
    "@react-native-community/eslint-config": "2.0.0",
    "@rnx-kit/align-deps": "^2.2.0",
    "@types/jest": "28.1.6",
    "@types/lodash": "4.14.182",
    "@types/mime-types": "2.1.1",
    "@types/node": "14.14.41",
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/react-native": "0.64.3",
    "@types/react-native-background-timer": "2.0.0",
    "@types/react-native-share-menu": "5.0.2",
    "@types/react-native-sqlite-storage": "6.0.0",
    "@types/react-test-renderer": "18.0.0",
    "@types/uuid": "^9.0.1",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "7.14.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.73.7",
    "path": "0.12.7",
    "prettier": "2.8.4",
    "prettier-plugin-x": "0.0.10",
    "react-native-svg-app-icon": "0.6.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "core-windows",
        "filesystem",
        "gestures",
        "hooks",
        "jest",
        "navigation/native",
        "navigation/stack",
        "netinfo",
        "react",
        "react-dom",
        "react-test-renderer",
        "safe-area",
        "screens",
        "sqlite",
        "svg"
      ]
    }
  },
  "reactNativePermissionsIOS": [
    "Camera",
    "FaceID",
    "MediaLibrary",
    "Microphone",
    "PhotoLibrary",
    "PhotoLibraryAddOnly"
  ]
}
